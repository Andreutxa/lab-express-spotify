<div>
    <h2>Title</h2>
    <h2>Listen</h2>
</div>

<div>

    {{#each tracks}}
        <h5>{{this.name}}</h5>
        <div>
            <figure>
                <audio
                    controls
                    src="{{this.preview_url}}">
                        Your browser does not support the
                        <code>audio</code> element.
                </audio>
            </figure>
        </div>
    {{/each}}
    
</div>
