<div class="card" style="width: 18rem;">

    {{#each artist}}

        <img src="{{this.images.0.url}}" class="card-img-top" alt="Artist image">
        <div class="card-body">
            <h5 class="card-title">{{this.name}}</h5>
            <a href="/albums/{{this.id}}" class="btn btn-primary">View albums</a>
        </div>
  
    {{/each}}

</div>